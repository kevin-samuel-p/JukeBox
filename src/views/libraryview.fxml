<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.geometry.Insets?>

<ScrollPane fitToWidth="true" fitToHeight="true"  
			xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.LibraryController" 
			stylesheets="@libraryview.css">
	<content>
		<TilePane fx:id="libraryTilePane" hgap="20" vgap="20" styleClass="library-tile-pane">
			<padding>
				<Insets bottom="20" right="20" top="20" left="20" />
			</padding>
		</TilePane>
    </content>
</ScrollPane>