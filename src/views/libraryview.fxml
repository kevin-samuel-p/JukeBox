<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<ScrollPane fitToWidth="true" fitToHeight="true"  
			xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.LibraryController" 
			stylesheets="@/styles/libraryview.css">
	<content>
		<VBox>
			<Label text="My Music" styleClass="header-label" />
			<FlowPane fx:id="libraryFlowPane" hgap="20" vgap="20" styleClass="library-flow-pane">
				<padding>
					<Insets bottom="20" right="20" top="20" left="20" />
				</padding>
			</FlowPane>
		</VBox>
    </content>
</ScrollPane>